#include "../config.h"
CC = @CC@
CCOPTIONS = @CFLAGS@ @LDFLAGS@
STD_INCLUDES = -I../ -I/usr/local/include

BASE_OBJS = image.o load_bmp.o load_anim.o

#ifdef HAVE_LIBJPEG
JPEG_OBJS = load_jpeg.o
#endif

#ifdef HAVE_LIBXPM
XPM_OBJS = load_xpm.o
#endif

#ifdef HAVE_LIBPNG
PNG_OBJS = load_png.o
#endif

OBJS = $(BASE_OBJS) $(JPEG_OBJS) $(XPM_OBJS) $(PNG_OBJS)

SRCS = $(OBJS:.o=.c)

NormalLibraryTarget(image,$(OBJS))

